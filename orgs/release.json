{
  "orgName": "demo-salesforce-servicecloud - Release Test Org",
  "edition": "Enterprise",
  "features": [
    "LightningServiceConsole",
    "LiveAgent",
    "LiveMessage",
    "ServiceCloud"
  ],
  "settings": {
    "lightningExperienceSettings": {
      "enableS1DesktopEnabled": true
    },
    "chatterSettings": {
      "enableChatter": true
    },
    "userManagementSettings": {
      "enableNewProfileUI": true
    },
    "securitySettings": {
      "enableAdminLoginAsAnyUser": true,
      "sessionSettings": {
        "forceRelogin": false
      }
    },
    "languageSettings": {
      "enableTranslationWorkbench": true
    },
    "omniChannelSettings": {
      "enableOmniChannel": true,
      "enableOmniSecondaryRoutingPriority": true,
      "enableOmniSkillsRouting": true
    },
    "liveAgentSettings": {
      "enableLiveAgent": true,
      "enableQuickTextEnabled": true
    },
    "caseSettings": {
      "closeCaseThroughStatusChange": true,
      "enableCaseFeed": true
    }
  }
}